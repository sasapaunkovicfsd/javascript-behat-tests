{
    "name": "dmore/chrome-mink-driver",
    "type": "library",
    "description": "Mink driver for controlling chrome without selenium",
    "license": "MIT",
    "authors": [
        {
            "name": "Dorian More",
            "email": "doriancmore@gmail.com"
        }
    ],
    "require": {
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "behat/mink": "^1.7",
        "textalk/websocket": "^1.2.0"
    },
    "require-dev": {
        "mink/driver-testsuite": "dev-master",
        "phpunit/phpunit": "^5.0.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "DMore\\ChromeDriver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DMore\\ChromeDriverTests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/adorin/driver-testsuite.git"
        }
    ],
    "minimum-stability": "stable"
}
